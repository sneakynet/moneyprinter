{% extends "../base.p2" %}

{% block title %}Equipment{% endblock %}

{% block content %}
<div>
  <table>
    <tr>
      <th>Name</th>
      <th>Port</th>
      <th>Wirecenter</th>
      <th>Description</th>
    </tr>
    <tr>
      <td>{{ equipment.Name }}</td>
      <td>{{ equipment.Port }}</td>
      <td><a href="/ui/wirecenters/{{ equipment.Wirecenter.ID }}">{{ equipment.Wirecenter.Name }}</a></td>
      <td>{{ equipment.Description }}</td>
    </tr>
  </table>
</div>
{% endblock %}
